<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="ke.co.appslab.home.views.HomeFragment"
        app:moduleName="home"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home"/>

    <action
        android:id="@+id/action_homeFragment_toAboutFragment"
        app:destination="@id/aboutFragment"/>
    <action
        android:id="@+id/action_homeFragment_toUsersFragment"
        app:destination="@id/users_nav_graph"/>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="ke.co.appslab.about.views.AboutFragment"
        app:moduleName="about"
        android:label="AboutFragment"
        tools:layout="@layout/fragment_about"/>

    <include app:graph="@navigation/users_nav_graph"/>


</navigation>